{
  "name": "ngx-state-traverser",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod nst-test",
    "build:lib": "ng build --prod ngx-state-traverser && cp ./README.md ./dist/ngx-state-traverser/",
    "test": "jest",
    "test:ci": "jest --runInBand --ci",
    "lint": "ng lint",
    "test:watch": "jest --watchAll",
    "get_version": "cat ./projects/ngx-state-traverser/package.json | grep version  | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "13.0.2",
    "@ngrx/store": "13.0.2",
    "angular-traversal": "latest",
    "rxjs": "~6.5.4",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.10",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.2",
    "@angular/language-service": "13.2.2",
    "@ngrx/store-devtools": "13.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "codelyzer": "6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "mrs-developer": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "ts-node": "9.1.1",
    "tslint": "~6.1.0",
    "typescript": "4.5.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "projects/ngx-state-traverser"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}
